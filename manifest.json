{
    "manifest_version": 3,
    "name": "geiot prototype",
    "version": "1.0.0",
    "action": { "default_popup": "index.html", "default_icon": "public/icon.png" },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "run_at": "document_end"
        }
      ]
}